###########################################################################
# General parameters
###########################################################################

name = c180

###########################################################################
# Polynomial selection
###########################################################################

tasks.polyselect.degree = 5

tasks.polyselect.P = 5000000
tasks.polyselect.admax = 7e6
tasks.polyselect.adrange = 5e3
tasks.polyselect.incr = 60
tasks.polyselect.nq = 1000
tasks.polyselect.nrkeep = 200

###########################################################################
# Sieve
###########################################################################

tasks.I = 15
tasks.qmin = 45000000
tasks.lim0 = 167874892
tasks.lim1 = 91296860
tasks.lpb0 = 31
tasks.lpb1 = 32
tasks.sieve.mfb0 = 62
tasks.sieve.mfb1 = 99
tasks.sieve.ncurves0 = 15
tasks.sieve.ncurves1 = 11

tasks.sieve.qrange = 5000

###########################################################################
# Filtering
###########################################################################

tasks.filter.purge.keep = 160
tasks.filter.target_density = 170.0

###########################################################################
# Linear algebra
###########################################################################

tasks.linalg.bwc.interleaving = 0
tasks.linalg.m = 64
tasks.linalg.n = 64

###########################################################################
# Characters
###########################################################################

tasks.linalg.characters.nchar = 50
