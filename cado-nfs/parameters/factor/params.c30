tasks.polyselect.adrange = 2500
tasks.polyselect.P = 1000
tasks.polyselect.nq = 3
tasks.polyselect.nrkeep = 2
tasks.polyselect.ropteffort = 1

tasks.I = 9
tasks.lim0 = 30000
tasks.lim1 = 30000
tasks.lpb0 = 17
tasks.lpb1 = 17
tasks.sieve.mfb0 = 18
tasks.sieve.mfb1 = 18

tasks.qmin = 50000
tasks.sieve.qrange = 1000
tasks.sieve.rels_wanted = 30000

tasks.linalg.m = 64
tasks.linalg.n = 64
tasks.linalg.characters.nchar = 50
