# those parameters are experimental

###########################################################################
#     Parameter file for Cado-NFS
###########################################################################
# See params/params.c90 for an example which contains some documentation.

###########################################################################
# General parameters
###########################################################################

name = c310

###########################################################################
# Polynomial selection
###########################################################################

tasks.polyselect.degree = 6

tasks.polyselect.P = 10000000
tasks.polyselect.admax = 2e17
tasks.polyselect.adrange = 14898723840
tasks.polyselect.incr = 465585120
tasks.polyselect.nq = 1296 # this is 6^4
tasks.polyselect.nrkeep = 100
tasks.wutimeout = 24000 # required for rootsieve in degree 6

###########################################################################
# Sieve
###########################################################################

tasks.lim0 = 2147483647 # cannot exceed 2^31-1 due to bug #21094
tasks.lim1 = 2147483647 # cannot exceed 2^31-1 due to bug #21094
tasks.lpb0 = 38
tasks.lpb1 = 40
tasks.sieve.mfb0 = 76
tasks.sieve.mfb1 = 120
tasks.sieve.ncurves0 = 30
tasks.sieve.ncurves1 = 40
tasks.I = 20

tasks.qmin = 1073741823

tasks.sieve.qrange = 10000

###########################################################################
# Filtering
###########################################################################

tasks.filter.purge.keep = 160
tasks.filter.target_density = 200

###########################################################################
# Linear algebra
###########################################################################

tasks.linalg.bwc.interleaving = 0
tasks.linalg.m = 64
tasks.linalg.n = 64

###########################################################################
# Characters
###########################################################################

tasks.linalg.characters.nchar = 50
